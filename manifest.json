{
  "name": "浏览器插件",
  "description": "这是第一个浏览器插件",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_title": "插件哦"
  },
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "notifications",
    "cookies",
    "tabs",
    "history",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["http://dev.a.newrank.cn/*"],
      "css": ["css/content.css"],
      "js": ["js/content.js"],
      "exclude_globs": [
        "http://dev.a.newrank.cn/media/*"
      ]
    }
  ],
  "homepage_url": "https://newrank.cn"
}